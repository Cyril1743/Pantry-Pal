[
    {
      "name": "Mac N Cheese",
      "description":"Delectable mac n cheese",
      "servings":2,
      "ingredients":[
            {
                "ingredientName":"macaroni",
                "ingredientAmount":2,
                "ingredientUnit":"cups"
            },
            {
                "ingredientName":"butter",
                "ingredientAmount":3,
                "ingredientUnit":"TBS"
            },
            {
                "ingredientName":"flour",
                "ingredientAmount":2,
                "ingredientUnit":"TBS"
            },
            {
                "ingredientName":"salt",
                "ingredientAmount":0.5,
                "ingredientUnit":"tsp"
            },
            {
                "ingredientName":"whole milk",
                "ingredientAmount":1.5,
                "ingredientUnit":"cups"
            },
            {
                "ingredientName":"cheddar cheese",
                "ingredientAmount":2.5,
                "ingredientUnit":"cups"
            }
        ],
        "steps": [
            {
                "order":1,
                "stepText":"Preheat oven to 350 degrees F and lightly grease a square baking dish."
            },
            {
                "order":2,
                "stepText":"Add macaroni to boiling water. Cook until al dente (about 10 mins, or according to package instructions)."
            },
            {
                "order":3,
                "stepText":"Melt butter in a saucepan over medium heat. Blend in flour and salt. Cook for 2 mins."
            },
            {
                "order":4,
                "stepText":"Reduce heat to medium low and add milk to butter mixture. Stir constantly for 2 mins. Remove from heat and stir in 1 cup of the cheddar cheese."
            },
            {
                "order":5,
                "stepText":"Stir cooked macaroni in to cheese mixture. Pour in to prepared baking dish. Top with remaining cheese."
            },
            {
                "order":6,
                "stepText":"Bake for 15 mins. Serve."
            }
        ]
    },
